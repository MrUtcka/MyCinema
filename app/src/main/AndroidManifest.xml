<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycinema">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ico"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyCinema">
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewActivity"></activity>
        <activity android:name=".PayActivity"></activity>
        <activity android:name=".AccountActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".AddActivity"></activity>
        <activity android:name=".ScheduleActivity"></activity>
        <activity android:name=".InfoActivity"></activity>
    </application>

</manifest>